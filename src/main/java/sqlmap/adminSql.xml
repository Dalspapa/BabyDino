<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dino.admin">

	<!-- admin report List -->
	<select id="adminrpList" resultType="dino.dto.ReportDto">
		SELECT
			*
		FROM
			d_report
		ORDER BY
			idx desc
	</select>
	
	<!-- 회원 강제탈퇴 -->
	<update id="adminMemberOut" parameterType="Integer">
		UPDATE
   			D_MEMBER
   		SET
   			MEMBER_TYPE = 9
   		WHERE
   			IDX = #{IDX}
	</update>
</mapper>