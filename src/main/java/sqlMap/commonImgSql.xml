<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dino.commonimg">
	<insert id="setKidimg" parameterType="dino.dto.Common_ImgDto">
		insert into
			d_common_img
		values(
			d_common_img_idx.nextval,
			#{ c_imgpath },
			2,
			2,
			1
		)		
	</insert>
</mapper>